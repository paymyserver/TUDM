    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/ccm_songremoval_title_height"
        android:background="@color/ccm_songremove_title_background"
        android:orientation="horizontal">

        <include
            android:id="@+id/include1"
            layout="@layout/tudm_header_layout"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/ccm_songremoval_title_height"
